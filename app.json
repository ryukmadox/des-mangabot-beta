{build:
  docker:
    worker: Dockerfile
run:
  worker: python bot.py

      "name": "mangabot",
      "description": "An api themed manga downloading bot.",
      "keywords": [
         "telegram",
         "anime",
         "manga",
         "api",
         "manga-bot"
      ],   
   "repository": "https://github.com/likucs/SaitamaRobot",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      
      }
      
   }
}
